<h2>ROSTER</h2>
<table class="table table-striped">
    <tr>
        <td>Name</td>
        <td>Gender</td>
        <td>Good Kicker?</td>
        <td>P1</td>
        <td>P2</td>
        <td>P3</td>
        <td>P4</td>
        <td>P5</td>
        <td>P6</td>
        <td>P7</td>
        <td>P8</td>
        <td>P9</td>
        <td>P10</td>
        <td>Active</td>
        <td>Actions</td>
    </tr>
    <% @players.each do |p| %>
    <tr>
        <td><%= p.name %></td>
        <td><%= p.gender %></td>
        <td><%= p.good_kicker %></td>
        <td><%= p.p1 %></td>
        <td><%= p.p2 %></td>
        <td><%= p.p3 %></td>
        <td><%= p.p4 %></td>
        <td><%= p.p5 %></td>
        <td><%= p.p6 %></td>
        <td><%= p.p7 %></td>
        <td><%= p.p8 %></td>
        <td><%= p.p9 %></td>
        <td><%= p.p10 %></td>
        <td><%= p.active %></td>
        <td><%= link_to "Edit", edit_player_path(p) %> | <%= link_to "Remove", player_path(p), method: :delete %></td>
    </tr>
    <% end %>
</table>
<br></br>